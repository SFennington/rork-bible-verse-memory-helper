{
  "cli": {
    "version": ">=16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "developmentClient": false,
        "credentialsSource": "remote",
        "releaseChannel": "production",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "alpha",
        "releaseStatus": "draft"
      }
    }
  }
}


